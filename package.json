{
  "name": "react-food",
  "version": "1.0.0",
  "author": "MiracleHorizon",
  "description": "Application for ordering food and products",
  "private": true,
  "scripts": {
    "start:frontend:prod": "cd client && yarn start",
    "start:frontend:dev": "cd client && yarn dev",
    "start:backend": "cd server && yarn start",
    "start:backend:prod": "cd server && yarn start:prod",
    "start:backend:dev": "cd server && yarn dev",
    "start:dev": "concurrently \"npm run start:backend:dev\" \"npm run start:frontend:dev\""
  },
  "dependencies": {
    "concurrently": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "prettier": "^3.0.0"
  }
}
